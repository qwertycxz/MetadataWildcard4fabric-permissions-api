			"contact": {
				"GitHub": "$github",
				"Ko-fi": "$kofi",
				"homepage": "$homepage"
			},
			"name": "$developer"
		}
	],
	"contact": {
		"issues": "$issue",
		"sources": "$scm"
	},
	"depends": {
		"fabric-permissions-api-v0": "*"
	},
